                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "campos": {
                                        "type": "array",
                                        "items": {
                                            "properties": {
                                                "resp": {
                                                    "type": "string"
                                                },
                                                "tipo": {
                                                    "type": "string",
                                                    "enum": [
                                                        "texto",
                                                        "textarea",
                                                        "select"
                                                    ]
                                                },
                                                "campo_id": {
                                                    "type": "integer"
                                                }
